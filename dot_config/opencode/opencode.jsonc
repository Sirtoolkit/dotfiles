{
  "$schema": "https://opencode.ai/config.json",
  "theme": "gruvbox",
  "plugin": [
    "opencode-antigravity-auth@latest"
  ],
  "agent": {
    "code-reviewer": {
      "description": "Reviews code for best practices and potential issues",
      "mode": "subagent",
      "model": "anthropic/claude-sonnet-4-20250514",
      "prompt": "You are a code reviewer. Focus on security, performance, and maintainability.",
      "tools": {
        "write": false,
        "edit": false
      }
    },
    "commits": {
      "description": "Creates git commits for configuration changes",
      "mode": "subagent",
      "model": "anthropic/claude-sonnet-4-20250514",
      "prompt": "You are a git commit agent. Analyze changes, create meaningful commits with descriptive messages following conventional commits format. Always verify before committing.",
      "tools": {
        "write": false,
        "edit": false
      }
    },
    "web-search": {
      "description": "Searches the web for information",
      "mode": "subagent",
      "model": "anthropic/claude-sonnet-4-20250514",
      "prompt": "You are a web search agent. Use the available search tools to find relevant, up-to-date information. Provide concise answers with sources.",
      "tools": {
        "write": false,
        "edit": false
      }
    }
  },
  "provider": {
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
