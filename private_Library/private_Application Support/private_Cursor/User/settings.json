{
  "workbench.colorCustomizations": {
    "statusBar.background": "#212121",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#3B3030",
    "statusBar.foreground": "#EBDBB2",
    "statusBar.debuggingForeground": "#EBDBB2"
  },
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "cloudcode.project": "crump-crm-app",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.compactFolders": false,
  "files.autoSave": "afterDelay",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "kotlin.languageServer.enabled": false,
  "kotlin.debugAdapter.enabled": false,
  "debug.javascript.autoAttachFilter": "always",
  "dart.devToolsLocation": {
    "default": "beside",
    "inspector": "sidebar"
  },
  "gitlens.ai.model": "gitkraken",
  "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "workbench.tree.indent": 20,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.folders.theme": "classic",
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.codeLensFontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "editor.inlayHints.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "editor.inlineSuggest.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "debug.console.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "chat.editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "notebook.output.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "scm.inputFontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "terminal.integrated.fontLigatures": true,
  "gitlens.currentLine.fontFamily": "'CaskaydiaCove Nerd Font', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-icon-theme.folders.color": "#45403D",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.navigationControl.enabled": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "window.customTitleBarVisibility": "never",
  "editor.lineNumbers": "relative",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.statusBarIconsEnabled": true,
  "dart.showDevToolsDebugToolBarButtons": false,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "off",
    "strings": "off"
  },
  "editor.parameterHints.enabled": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabCompletion": "off",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.toolBarLocation": "hidden",
  "breadcrumbs.enabled": false,
  "superdesign.aiModelProvider": "openrouter",
  "superdesign.aiModel": "claude-4-sonnet-20250514",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
